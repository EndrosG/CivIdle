      "@capawesome/capacitor-live-update": "^6.8.0",
      "@msgpack/msgpack": "^3.0.0-beta2",
      "@pixi/filter-outline": "^5.2.0",
      "@pixi/graphics-smooth": "^1.1.0",
      "@pixi/sound": "^5.2.0",
      "@sentry/browser": "^8.47.0",
      "@tippyjs/react": "^4.2.6",
      "classnames": "^2.3.1",
      "d3-geo": "^3.1.1",
      "fflate": "^0.8.2",
      "gameanalytics": "^4.4.7",
      "install": "^0.13.0",
      "memoizee": "^0.4.17",
      "npm": "^10.9.0",
      "pixi.js": "7.4.2",
      "randomcolor": "^0.6.2",
      "react": "^18.2.0",
      "react-dom": "^18.2.0",
      "react-modal": "^3.15.1",
      "react-virtuoso": "^4.7.11",
      "svg-pathdata": "^8.0.0",
      "tippy.js": "^6.3.7",
      "topojson-client": "^3.1.0",
      "uplot": "^1.6.30",
      "xp.css": "^0.3.0"
   },
