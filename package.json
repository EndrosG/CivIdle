      "@types/randomcolor": "^0.5.9",
      "@types/react": "^18.0.0",
      "@types/react-dom": "^18.0.8",
      "@types/react-modal": "^3.13.1",
      "@types/topojson-client": "^3.1.5",
      "@types/uuid": "^9.0.0",
      "@typescript/native-preview": "7.0.0-dev.20251009.1",
      "@vitejs/plugin-react-swc": "^3.6.0",
      "fs-extra": "^11.1.0",
      "gaze": "^1.1.3",
      "glob": "^8.0.3",
